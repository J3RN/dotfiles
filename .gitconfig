[core]
	excludesfile = ~/.gitignore_global
[user]
	name = Jonathan Arnett
	email = jonarnett90@gmail.com
	signingkey = 107E89E8
[push]
	default = simple
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[alias]
	co = checkout
	br = branch
	mkbr = checkout -b
	rmbr = branch -d
	mvbr = branch -m
	com = commit -S -m
	st = status -s
	pullbr = checkout --track
	lsbr = branch
	adda = add -A
	hist = log --oneline
	lsrm = remote -v
	deploy = push deploy
	switch = !legit switch \"$@\"
	branches = !legit branches
	sprout = !legit sprout \"$@\"
	unpublish = !legit unpublish \"$@\"
	harvest = !legit harvest \"$@\"
	sync = !legit sync \"$@\"
	publish = !legit publish \"$@\"
	graft = !legit graft \"$@\"
	pr = !hub pull-request
	browse = !hub browse
[color]
	ui = auto
[commit]
	gpgSign = true
